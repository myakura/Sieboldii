{
	"manifest_version": 3,
	"name": "Sieboldii",
	"version": "0.0.7",
	"author": "Masataka Yakura",
	"permissions": ["tabs", "contextMenus"],
	"background": {
		"service_worker": "sieboldii_background.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["open-props-1.5.6.min.css", "sieboldii_content.css"],
			"js": ["sieboldii_content.js"]
		}
	]
}
