{
	"manifest_version": 3,
	"name": "Sieboldii",
	"version": "0.0.2",
	"author": "Masataka Yakura",
	"permissions": ["tabs", "contextMenus"],
	"background": {
		"service_worker": "sieboldii_background.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["sieboldii_content.js"]
		}
	]
}
